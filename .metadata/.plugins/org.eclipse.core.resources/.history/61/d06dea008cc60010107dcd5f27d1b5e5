package core;

public class P004_Conditions {
	public static void main(String[] args) {
		// if{} else{}
		int a = 14, b = 14;
		if (a < b) {
			System.out.println("a is greater than b");
		} else {
			System.out.println("b is greater than a");
		}
		// nested if
		
		
		// else if ladder

		// switch case
	}
}
