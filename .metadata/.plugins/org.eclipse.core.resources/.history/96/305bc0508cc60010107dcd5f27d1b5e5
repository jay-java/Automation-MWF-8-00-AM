package core;

public class P004_Conditions {
	public static void main(String[] args) {
		// if{} else{}
		int a = 14, b = 13;
		if (a < b) {
			System.out.println("a is greater than b");
		} else {
			System.out.println("b is greater than a");
		}
		// nested if
		int age = 45;
		if(age > 18) {
			
		}
		
		else {
			System.out.println("age is less than 18");
		}
		// else if ladder

		// switch case
	}
}
