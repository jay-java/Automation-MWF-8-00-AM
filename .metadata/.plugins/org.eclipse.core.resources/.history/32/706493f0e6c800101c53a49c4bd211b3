package core;

public class P007_Prime {
	public static void main(String[] args) {
		int num = 14;
		int counter = 0;
		for (int i = 1; i <= num; i++) {
			if (num % i == 0) {
				counter++;
			}
		}
		if (counter == 2) {
			System.out.println(num + " is prime");
		} else {
			System.out.println(num + " is not prime");
		}
	}
}
