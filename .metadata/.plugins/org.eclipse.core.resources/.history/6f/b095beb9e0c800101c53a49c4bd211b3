package core;

public class P005_Loops {
	public static void main(String[] args) {
		// for loop
//		for(initialization;condition;incre/decre) {}
		System.out.println(1);
		System.out.println(2);
		System.out.println(3);
		System.out.println(4);
		System.out.println(5);

		// while loop
		// do while
		// for each
	}
}
